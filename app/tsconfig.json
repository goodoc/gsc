
  {
    "compilerOptions": {
      "outDir": "dist",
      "rootDir": ".",
      "lib": [
        "esnext"
      ],
      "esModuleInterop": true,
      "strict": false,
      "target": "es5",
      "allowJs": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true,
      "noEmit": true,
      "module": "commonjs",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve"
    },
    "include": [
      "."
    ],
    "exclude": [
      "node_modules"
    ]
  }
